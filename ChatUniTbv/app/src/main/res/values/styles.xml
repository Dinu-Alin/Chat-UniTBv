<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="start_style">
        <item name="android:textSize">36sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="BlackButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/shape_button_background_black</item>
    </style>
    <style name="WhiteButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/shape_button_background_white</item>
    </style>
    <style name="GreyButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/shape_button_background_gray</item>
    </style>
    <style name="MyOverflowButtonStyle" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/white</item>
    </style>
</resources>